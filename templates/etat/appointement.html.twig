        Pied de page
    </div>
    {#<p class="page">Page </p>#}
    {% endif %}
</div>
<div >
<div style="margin-top: -80px; padding-bottom: -10px; position: center; width: auto" class="row">
    <table width="100%" style="font-size: x-small">
        <tr>
            <th colspan="4">
                <p style="text-align: center; border: solid 1px; font-size: x-small">
                    <u><b>ASSURANCE SUNU VOYAGE</b></u> <br>
                    <b>Conditions Particulières  N° {{' ' ~ contrat.numpolice ~ contrat.produit.codeproduit}}</b>
                </p>
            </th>
        </tr>
        <tr><th colspan="4"><p style="text-align: center; text-transform: uppercase"><u>Souscripteur</u></p></th></tr>
        <tr>
            <td colspan="2">
                <u><b>Nom :</b></u> {{ ' ' ~ contrat.nomsouscr }}<br>
                <u><b>Prénom(s) :</b></u>{{ ' ' ~ contrat.prenomsouscr }}
            </td>
            <td colspan="2">
                {% set formattedString = contrat.telephonesouscr|split('', 2)|join(' ') %}
                <b>Tél :</b> {{ ' ' ~ formattedString[:5]|replace({' ': ''}) ~' '~ formattedString|slice(5,formattedString|length) }}<br>
                <b>E-Mail :</b> {{ ' ' ~ contrat.email }}
            </td>
        </tr>
        <tr>
            <td colspan="4" style="text-align: center; text-transform: uppercase">
                <b><u>Période de garantie</u></b>
            </td>
        </tr>
        <tr>
            <td>
                <b>Du :</b>
                {% if contrat.datecreated | date ('d/m/Y') is same as (contrat.datedebut | date ('d/m/Y')) %}
                {{ ' ' ~ contrat.datecreated | date ('d/m/Y H:m')}}
                {% else %}
                {{ ' ' ~ contrat.datedebut | date ('d/m/Y') ~ ' 00:00' }}
                {% endif %}
            </td>
            <td>
                <b>Au :</b> {{ ' ' ~ contrat.datefin | date('d/m/Y') ~ ' 23:59' }}
            </td>
            <td >
                <b>Durée :</b> {{ ' ' ~ contrat.duree ~ ' jours' }}
            </td>
            <td>
                <b>Zone :</b> {{ ' ' ~ contrat.produit }}
            </td>
        </tr>
        <tr><td colspan="4" style="text-align: center; text-transform: uppercase"><b><u>Assuré(e)</u></b></td></tr>
        <tr>
            <td colspan="2">
                <u><b>Nom :</b></u> {{ ' ' ~ contrat.client.nom }} <br>
                <u><b>Prénom(s) :</b></u> {{ ' ' ~ contrat.client.prenom }} <br>
                <b>Date de Naissance :</b> {{ ' ' ~ contrat.client.datenaissance | date('d/m/Y') }}
            </td>
            <td colspan="2">
                <b>Passeport N° :</b> {{ ' ' ~ contrat.numpiece }} <br>
                <b>Nationalité  :</b> {{ ' ' ~ contrat.nationalite }} <br>
                {% set formatted = contrat.client.telephone|split('', 2)|join(' ') %}
                <b>Tél          :</b> {{ ' ' ~formatted[:5]|replace({' ': ''}) ~' '~ formatted|slice(5,formatted|length) }}
            </td>
        </tr>
    </table>
