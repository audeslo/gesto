    {% if  contrat.createdBy.agence.partenaire.avecentete %}

    <img width="150px;" alt="Logo_sunu" src="data:image/png;base64,{{logo}}" />
    {#<img width="150px;" src="{{app.request.getSchemeAndHttpHost() ~  asset('templates/images/logo/logo_sunu.png') }}" alt="Logo_SUNU">#}
</div>
<div id="footer" >
